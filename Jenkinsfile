node{
    cleanWs()
    try{
        stage('01 - Stage'){
            sh "echo 'Hello World !'"
        }
        stage('02 - Stage'){
            sh "git repository name is : " + repository_name
        }
        stage('03 - Stage'){
            sh "author of commit is : " + commit_author
        }





    }
    finally{
        cleanWs()
    }


}